{
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react-jsx",
    "paths": {
      "App/*": ["app/*"],
      "Common/*": ["common/*"],
      "Contexts/*": ["contexts/*"],
      "Features/*": ["features/*"],
      "Redux/*": ["redux/*"],
      "Routes/*": ["routes/*"],
      "Styles/*": ["styles/*"],
      "Types/*": ["types/*"],
      "Util/*": ["util/*"],
    }
  },
  "extends": "../config/tsconfig.json",
  "include": ["src"],
  "types": ["@types"]
}
